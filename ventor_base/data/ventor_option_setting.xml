<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    <!-- Package management -->
        <record id="enable_over_flow" model="ventor.option.setting">
            <field name="name">enable_over_flow</field>
            <field name="is_set">False</field>
            <field name="action_type">package_management</field>
        </record>

        <record id="allow_validate_less" model="ventor.option.setting">
            <field name="name">allow_validate_less</field>
            <field name="is_set">True</field>
            <field name="action_type">package_management</field>
        </record>

        <record id="allow_manual_editing" model="ventor.option.setting">
            <field name="name">allow_manual_editing</field>
            <field name="is_set">True</field>
            <field name="action_type">package_management</field>
        </record>

        <record id="use_default_package_name" model="ventor.option.setting">
            <field name="name">use_default_package_name</field>
            <field name="is_set">True</field>
            <field name="action_type">package_management</field>
        </record>

        <!-- Batch picking -->
        <record id="validate_picking_batch" model="ventor.option.setting">
            <field name="name">validate_picking_batch</field>
            <field name="is_set">True</field>
            <field name="action_type">batch_picking</field>
        </record>

        <record id="confirm_source_location" model="ventor.option.setting">
            <field name="name">confirm_source_location</field>
            <field name="is_set">False</field>
            <field name="action_type">batch_picking</field>
        </record>

        <record id="change_source_location" model="ventor.option.setting">
            <field name="name">change_source_location</field>
            <field name="is_set">False</field>
            <field name="action_type">batch_picking</field>
        </record>

        <record id="confirm_product" model="ventor.option.setting">
            <field name="name">confirm_product</field>
            <field name="is_set">False</field>
            <field name="action_type">batch_picking</field>
        </record>
    </data>
</odoo>
